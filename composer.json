{
  "name": "agentur1601com/contimizer",
  "description": "Export / import seo title and description directly from backend",
  "type":"contao-module",
  "license":"LGPL-3.0+",
  "authors": [
    {
      "name"    : "Lukas Beck",
      "email"   : "lb@1601.com",
      "homepage"  : "https://github.com/LBeckX"
    }
  ],
  "require": {
    "php": ">=7.1",
    "contao/core-bundle":"^4.1"
  },
  "extra": {
    "branch-alias": {
      "master": "1.1.*-dev"
    },
    "contao-manager-plugin": "agentur1601com\\Contimizer\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "agentur1601com\\Contimizer\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/"
    ]
  }
}